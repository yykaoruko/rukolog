{
  "name": "rukolog",
  "version": "1.0.0",
  "author": "yykaoruko",
  "scripts": {
    "deploy": "run-s *:build",
    "dev": "nuxt-ts",
    "gen:c": "hygen init component",
    "gen:l": "hygen init log",
    "lint": "vue-cli-service lint",
    "lint:script": "vue-cli-service lint . --ext .vue,.ts,.js",
    "lint:script:staged": "vue-cli-service lint . --ext .vue,.ts,.js",
    "lint:script:nofix": "vue-cli-service lint . --ext .vue,.ts,.js --no-fix",
    "lint:style": "vue-cli-service lint:style app/**/*.{css,scss} app/layouts/**/*.vue app/pages/**/*.vue app/components/**/*.vue",
    "test:unit": "vue-cli-service test:unit --config ./jest.config.js",
    "test:unit:ci": "vue-cli-service test:unit --config ./jest.config.js --ci --maxWorkers=2",
    "md:build": "processmd ./app/logs/logs/markdown/**.md --stdout --outputDir ./app/logs/dist --summaryOutput ./app/contents/dist/summary.json",
    "generate:build": "nuxt-ts generate"
  },
  "main": "index.js",
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.1.5",
    "@vue/cli-service": "^3.11.0",
    "bootstrap": "^4.3.1",
    "nuxt": "^2.9.2"
  },
  "devDependencies": {
    "@ascendancyy/vue-cli-plugin-stylelint": "^1.1.2",
    "@nuxt/typescript-build": "^0.2.6",
    "@nuxtjs/style-resources": "^1.0.0",
    "@types/jest": "^24.0.18",
    "@typescript-eslint/parser": "^2.1.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "4.0.0-beta.3",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "dayjs": "^1.8.16",
    "eslint": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "husky": "^3.0.5",
    "hygen": "^4.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "processmd": "^4.4.2",
    "sass-loader": "^8.0.0",
    "stylelint": "^10.1.0",
    "stylelint-config-property-sort-order-smacss": "^5.0.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-order": "^3.1.0",
    "stylelint-scss": "^3.10.1",
    "ts-jest": "24.0.2",
    "typescript": "3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "yarn lint:script:staged",
      "git add"
    ]
  },
  "license": "MIT",
  "repository": "ssh://git@github.com/yykaoruko/rukolog.git"
}
